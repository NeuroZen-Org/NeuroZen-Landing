---
import Section from "../components/Section.astro";
import Psychologist from "../components/Psychologist.astro";
import { psychologists } from "@/consts/psychologists";
---

<Section
  title="Nuestros mejores psicólogos"
  description="Profesionales certificados listos para ayudarte a reducir el estrés, mejorar tu bienestar emocional y recuperar tu equilibrio."
>
  <div class="flex flex-col gap-9">
    {
      psychologists.map((psychologist) => (
        <Psychologist psychologist={psychologist} />
      ))
    }
  </div>
</Section>
